{
  "manifest_version": 2,
 
  "name": "Vyrtex-extenstion",
  "description": "Quick add to Vyrtex from Chrome",
  "version": "1.0",
 
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "identity",
   "activeTab", 
   "tabs", 
   "http://localhost:3000/*"
   ]
}